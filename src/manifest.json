{
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["listener.js"]
    }
  ],
  "description": "Allows circumventing x-frame-options within the Inline Webviewer module.",
  "manifest_version": 2,
  "name": "FoundryVTT Inline Webviewer",
  "permissions": ["webRequest", "webRequestBlocking", "<all_urls>", "browsingData"],
  "homepage_url": "https://github.com/ardittristan/FoundryVTT-Inline-Webviewer-Extension",
  "icons": {
    "48": "icon.png",
    "96": "icon.png"
  },
  "version": "1.0.3"
}
